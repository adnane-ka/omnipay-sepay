{
    "name": "adnane-ka/omnipay-sepay",
    "description": "Sepay gateway for Omnipay payment processing library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\Sepay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adnane Kadri",
            "email": "dannykadri0@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "omnipay/common": "^3.3",
        "symfony/http-client": "^7.1",
        "php-http/httplug": "^2.4",
        "nyholm/psr7": "^1.8",
        "php-http/curl-client": "^2.3",
        "guzzlehttp/psr7": "^2.7",
        "php-http/message": "^1.16"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
